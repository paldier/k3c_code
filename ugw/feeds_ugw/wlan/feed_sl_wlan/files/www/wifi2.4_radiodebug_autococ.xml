<?xml version="1.0" encoding="UTF-8"?>
<view>
  <header>
         <image>images/icons/wireless.png</image>
    <name>Advanced</name>
    <description>Advanced settings for the wireless LAN interfaces that operate in the 2.4GHz range. Typically you shouldn't need to change these settings.</description>
  </header>
  <object viewtype="form">    
    <object name="Device.WiFi.Radio.1.X_LANTIQ_COM_Vendor">
      <row>
        <col>
          <parameter name="AutoCocEnabled" required="false" type="toggle" webname="Auto CoC"/>
        </col>
        <col>
          <parameter name="NumOfAntennas" required="false" type="number" webname="Number of Antennas"/>
        </col>
      </row>
      <row>
        <col>
          <parameter name="WaveAutoCoC1x1TimerInterval" required="false" type="number" webname="Auto CoC Timer Interval 1x1 in 100ms"/>
        </col>
        <col>
          <parameter name="WaveAutoCoC2x2TimerInterval" required="false" type="number" webname="Auto CoC Timer Interval 2x2 in 100ms"/>
        </col>
      </row>
      <row>
        <col>
          <parameter name="WaveAutoCoC3x3TimerInterval" required="false" type="number"  webname="Auto CoC Timer Interval 3x3 in 100ms"/>
        </col>
          <col>
          <parameter name="WaveAutoCoC4x4TimerInterval" required="false" type="number"  webname="Auto CoC Timer Interval 4x4 in 100ms"/>
        </col>
      </row>
        <row>
        <col>
          <parameter name="WaveAutoCoC1x1HighLimit" required="false" type="number"  webname="Auto CoC 1x1 High Limit in bytes/s"/>
        </col>
          <col>
          <parameter name="WaveAutoCoC2x2LowLimit" required="false" type="number" webname="Auto CoC 2x2 Low Limit in bytes/s"/>
        </col>
      </row>
        <row>
        <col>
          <parameter name="WaveAutoCoC2x2HighLimit" required="false" type="number" webname="Auto CoC 2x2 High Limit in bytes/s"/>
        </col>
          <col>
          <parameter name="WaveAutoCoC3x3LowLimit" required="false" type="number"  webname="Auto CoC 3x3 Low Limit in bytes/s"/>
        </col>
      </row>
        <row>
        <col>
          <parameter name="WaveAutoCoC3x3HighLimit" required="false" type="number" webname="Auto CoC 3x3 High Limit in bytes/s"/>
        </col>
          <col>
          <parameter name="WaveAutoCoC4x4LowLimit" required="false" type="number" webname="Auto CoC 4x4 Low Limit in bytes/s"/>
        </col>
      </row>
    </object>
    <parameter name="Modify" type="submitbutton" routeurl="Apply($event)" webname="Apply"/>
  </object>
</view>
